# more read
mov $0x023f,%ax # command 2, 0x3f sectors 31Â½K
mov $0x0101,%cx # from cylinder 2 sector 1 (sector starts from 1, not 0)
# %dl (drive number) and %dh (head) is unchanged
mov $0x0bc0,%bx # to es:bx=bc00:0000 (7e00+1f*200)
mov %bx,%es
xor %bx,%bx
int $0x13 # read disk


