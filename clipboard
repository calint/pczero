##-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- - irq 8 timer
# disable PIC
mov $0xff,%al
out %al,$0xa1
out %al,$0x21

# enable 1024Hz timer
mov $0x8b,%al
out %al,$0x70
in $0x71,%al
mov %al,%dl
mov $0x8b,%al
out %al,$0x70
or $0x40,%dl
mov %dl,%al
out %al,$0x71
